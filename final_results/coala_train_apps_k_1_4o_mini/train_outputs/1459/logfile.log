def calculate_perimeter(N, M, points):
    transformed_points = []
    # Transform the points based on the slope M
    for x, y in points:
        new_x = x - M * y  # Rotate the point
        new_y = M * x + y  # Rotate the point
        transformed_points.append((new_x, new_y))
    
    # Find the min and max x and y coordinates in the transformed space
    min_x = min(transformed_points, key=lambda p: p[0])[0]
