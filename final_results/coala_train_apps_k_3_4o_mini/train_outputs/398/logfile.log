            for i in range(len(A)):
                prefix_sum[i + 1] = prefix_sum[i] + A[i]
        
            min_length = float('inf')
            dq = deque()
        
            for i in range(len(prefix_sum)):
                while dq and prefix_sum[i] - prefix_sum[dq[0]] >= K:
