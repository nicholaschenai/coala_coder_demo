class Solution:
    def longestArithSeqLength(self, A: list[int]) -> int:
        # Check if the input list is empty
        if not A:
            return 0
        
        n = len(A)  # Get the length of the input list
        dp = {}  # Dictionary to store lengths of subsequences
        max_length = 0  # Variable to track the maximum length
        
        # Iterate through the list to fill the dp dictionary
        for i in range(n):
            for j in range(i):
                difference = A[i] - A[j]  # Calculate the difference
                # Check if there is an existing sequence with this difference
                if (j, difference) in dp:
                    dp[(i, difference)] = dp[(j, difference)] + 1  # Extend the sequence
                else:
                    dp[(i, difference)] = 2  # Start a new sequence with length 2
                
                # Update the maximum length found
                max_length = max(max_length, dp[(i, difference)])
        
        return max_length  # Return the maximum length of arithmetic subsequence
```
