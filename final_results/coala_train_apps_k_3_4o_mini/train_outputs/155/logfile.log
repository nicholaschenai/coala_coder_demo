                            max_count = max(max_count, 1 + dfs(index - x, visited))
            
            return max_count

        max_jumps = 0
        for index in range(len(arr)):
            visited = set()  # Reset visited for each starting index
            max_jumps = max(max_jumps, dfs(index, visited))  # Update max jumps
