class Solution:
    def mincostTickets(self, days: list[int], costs: list[int]) -> int:
        # Initialize a cost array with size 366 (to cover days 1 to 365)
        cost = [0] * 366  # Cost for day 0 is 0
        
        # Set of travel days for quick lookup
        travel_days = set(days)
        
        # Iterate through each day of the year
        for day in range(1, 366):
            if day in travel_days:
                # Calculate the minimum cost for each type of ticket
                cost[day] = min(cost[day - 1] + costs[0],  # 1-day pass
                                cost[max(0, day - 7)] + costs[1],  # 7-day pass
                                cost[max(0, day - 30)] + costs[2])  # 30-day pass
            else:
                # If it's not a travel day, carry forward the cost from the previous day
                cost[day] = cost[day - 1]
        
        # The answer is the minimum cost to cover all travel days
        return cost[days[-1]]

