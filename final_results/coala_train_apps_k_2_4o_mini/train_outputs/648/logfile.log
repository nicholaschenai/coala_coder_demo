def jump_to_hill(heights, start_index, jumps):
    # Start from the given hill index
    current_index = start_index
    for jump in range(jumps):
        next_index = current_index + 1
        found = False
        # Look for the next higher hill within the next 100 hills
        while next_index < len(heights) and next_index <= current_index + 100:
            if heights[next_index] > heights[current_index]:
                current_index = next_index
                found = True
                break
            next_index += 1
        if not found:
            break
    return current_index

def update_heights(heights, L, R, X):
    # Update the heights of hills from L to R by X
    for i in range(L-1, R):
        heights[i] += X

